Xft.dpi: 120.0
xterm*faceName: Iosevka:style=Medium:size=12:antialias=true 
!xterm*faceName: Terminus:style=Regular:size=11:antialias=false
!xterm*facename: GohuFont:style=Medium:size=11:antialias=false
!xterm*transparency: true

URxvt*font: xft:Iosevka:style=Medium:size=12:antialias=true 
URxvt*borderless: 1
URxvt*scrollBar: false
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*perl-ext-common: resize-font,clipboard,keyboard-select
URxvt*keysym.M-Escape: perl:keyboard-select:activate
URxvt*keysym.M-c: perl:clipboard:copy
URxvt*keysym.M-v: perl:clipboard:paste
URxvt*keysym.M-C-v: perl:clipboard:paste_escaped

